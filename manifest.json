{
	"manifest_version": 2,
	"name": "WP Table Exporter",
	"description": "WordPress Table Exporter",
	"version": "1.0",
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/wp-admin/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"permissions": [
		"webNavigation"
	]
}